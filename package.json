{
  "name": "@jeportie/create-checks",
  "version": "0.0.0-semantically-released",
  "description": "Scaffolding CLI that installs ESLint + Prettier and injects config into any project",
  "author": "jeportie",
  "repository": {
    "type": "git",
    "url": "git://github.com/jeportie/create-checks.git"
  },
  "keywords": [
    "eslint",
    "prettier",
    "scaffolding",
    "create",
    "cli"
  ],
  "license": "MIT",
  "private": false,
  "type": "module",
  "bin": {
    "create-checks": "src/index.js"
  },
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier . --write",
    "lint": "eslint .",
    "secretlint": "secretlint ./src --maskSecrets",
    "spellcheck": "cspell --no-progress \"./**/*.{js,mjs,cjs,md,json,yml}\"",
    "test": "vitest --run",
    "test:coverage": "vitest --coverage --run",
    "test:integration": "vitest int --run",
    "prepare": "husky || true"
  },
  "dependencies": {
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "inquirer": "~10.2.2",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.0.0",
    "@secretlint/secretlint-rule-pattern": "^8.2.4",
    "@secretlint/secretlint-rule-preset-recommend": "^8.2.4",
    "@vitest/coverage-v8": "^2.0.4",
    "commitlint-plugin-cspell": "^0.1.1",
    "conventional-changelog-conventionalcommits": "~8.0.0",
    "cspell": "^8.12.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "secretlint": "^8.2.4",
    "semantic-release": "~24.0.0",
    "vitest": "^2.0.4"
  }
}
